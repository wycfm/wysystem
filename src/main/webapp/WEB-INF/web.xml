<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>wysystem</display-name>
  
  <servlet>
  	<servlet-name>coreAction</servlet-name>
  	<servlet-class>cn.wycfm.core.action.CoreAction</servlet-class>
  	<load-on-startup>1</load-on-startup>
  </servlet>
  
  
  <servlet>
  	<servlet-name>blogAction</servlet-name>
  	<servlet-class>cn.wycfm.blog.action.BlogAction</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>coreAction</servlet-name>
  	<url-pattern>/</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>blogAction</servlet-name>
  	<url-pattern>/blog/</url-pattern>
  </servlet-mapping>
</web-app>
